# Git Registry Configuration
# Copy this file to .env and fill in your values

# Git repository URL
# Examples:
#   GIT_REGISTRY_URL=https://github.com/user/repo
#   GIT_REGISTRY_URL=file:///tmp/local-repo
GIT_REGISTRY_URL=

# Registry name to use in ARM
# Example: GIT_REGISTRY_NAME=my-registry
GIT_REGISTRY_NAME=
