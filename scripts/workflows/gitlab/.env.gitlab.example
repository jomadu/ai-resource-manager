# GitLab Registry Configuration
# Copy this file to .env and fill in your values

# GitLab instance URL
# Example: https://gitlab.example.com
GITLAB_URL=

# GitLab project ID containing the rule packages
GITLAB_PROJECT_ID=

# GitLab API token with read access to packages
GITLAB_TOKEN=

# GitLab API version (default: v4)
GITLAB_API_VERSION=v4

# Ruleset name to install
# Example: ai-rules
RULESET_NAME=

# Include patterns for files to install
# Example: "*.yml" or "rules/*.md"
INCLUDE_PATTERNS=

# Target sinks (comma-separated)
# Example: cursor,q
SINKS=
